    "description": "A simple Chrome extension for integration of ChatGPT.",
    "permissions": ["storage", "activeTab" , "scripting"],
    "content_scripts": [
      {
        "matches": ["https://twitter.com/*"],
        "js": ["content.js"]
      }
    ]
  }
  